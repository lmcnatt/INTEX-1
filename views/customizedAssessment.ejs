                    <h6 id="depressionA" class="hidden">Hey</h6>
                    <h6 id="depressionB" class="hidden">Hey</h6>
                <h3 id="apathy" class="hidden">&bull; Apathy</h3>
                    <h6 id="apathyA" class="hidden">Hey</h6>
                    <h6 id="apathyB" class="hidden">Hey</h6>
                <h3 id="sleepIssues" class="hidden">&bull; Sleep Issues</h3>
                    <h6 id="sleepIssuesA" class="hidden">Hey</h6>
                    <h6 id="sleepIssuesB" class="hidden">Hey</h6>
            </div>
