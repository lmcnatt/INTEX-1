    <script>
        let userAge = parseInt(document.getElementById("ageTag").innerHTML)

        if (userAge >= 59){
            userAge = "Baby Boomers"
            document.getElementById("getHelpButton").action = ("/helpBoomers")
            document.getElementById("stat1").innerHTML = "&bull; Apathy"
            document.getElementById("stat2").innerHTML = "&bull; Sleep Issues"
            document.getElementById("stat3").innerHTML = "&bull; Being Highly Distracted"

        }
        else if (userAge >= 43){
            userAge = "Gen X"
            document.getElementById("getHelpButton").action = ("/helpGenX")
            document.getElementById("stat1").innerHTML = "&bull; Being Highly Distracted"
            document.getElementById("stat2").innerHTML = "&bull; Worry"
            document.getElementById("stat3").innerHTML = "&bull; Sleep Issues"
        }
        else if (userAge >= 27){
            userAge = "Millennials"
            document.getElementById("getHelpButton").action = ("/helpMillenials")
            document.getElementById("stat1").innerHTML = "&bull; Worry"
            document.getElementById("stat2").innerHTML = "&bull; Being Highly Distracted"
            document.getElementById("stat3").innerHTML = "&bull; Difficulty Conscentrating"
        }
        else{
            userAge = "Gen Z"
            document.getElementById("getHelpButton").action = ("/helpGenZ")
            document.getElementById("stat1").innerHTML = "&bull; Worry"
            document.getElementById("stat2").innerHTML = "&bull; Depression"
            document.getElementById("stat3").innerHTML = "&bull; Being Highly Distracted"
        }

        let sGenerationalAssessment = userAge + ",<br>"
        document.getElementById("GenerationAssessment").innerHTML = sGenerationalAssessment
    </script>
    
    
